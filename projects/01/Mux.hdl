/** 
 * Multiplexor:
 * if (sel == 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Nand(a=w1,b=w2,out=out);
    Nand(a=notsel,b=a,out=w1);
    Nand(a=sel,b=b,out=w2);
    Not(in=sel,out=notsel);
}
